<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android"
           xmlns:androidprv="http://schemas.android.com/apk/prv/res/android">

    <style name="TextAppearance.QS.Status.Build">
        <item name="android:textColor">@*android:color/transparent</item>
    </style>

    <style name="ChargingAnim.WallpaperBackground">
        <item name="chargingAnimColor">?android:attr/colorAccent</item>
        <item name="android:textColor">?android:attr/colorAccent</item>
    </style>

    <style name="ChargingAnim.DarkBackground">
        <item name="chargingAnimColor">?android:attr/colorAccent</item>
        <item name="android:textColor">?android:attr/colorAccent</item>
    </style>

    <style name="Theme.SystemUI.Dialog" parent="@style/Theme.SystemUI.DayNightDialog">
        <item name="android:buttonCornerRadius">@*android:dimen/config_dialogCornerRadius</item>
        <item name="android:colorBackground">?android:attr/colorBackgroundFloating</item>
    </style>

    <style name="QSCustomizeToolbar" parent="@*android:style/Widget.DeviceDefault.Toolbar">
        <item name="android:elevation">0dp</item>
    </style>

    <style name="TrimmedHorizontalProgressBar"
           parent="android:Widget.Material.ProgressBar.Horizontal">
        <item name="android:minHeight">1.5dp</item>
        <item name="android:maxHeight">1.5dp</item>
    </style>

    <style name="LockPatternStyle">
        <item name="*android:dotColor">?android:attr/textColorPrimary</item>
    </style>

    <style name="LockPatternStyleBiometricPrompt">
        <item name="*android:dotColor">?android:attr/textColorPrimary</item>
    </style>

    <style name="Theme.SystemUI.QuickSettings" parent="@*android:style/Theme.DeviceDefault">
        <item name="offStateColor">@*android:color/surface_dark_alpha_45</item>
    </style>

</resources>
